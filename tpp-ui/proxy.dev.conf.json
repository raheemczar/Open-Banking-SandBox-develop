{
  "/ledgers/*": {
    "target": "https://dev-modelbank-ledgers.cloud.adorsys.de",
    "secure": false,
    "changeOrigin": true,
    "logLevel": "debug",
    "pathRewrite": {
      "^/ledgers": ""
    }
  },

  "/certgen/*": {
    "target": "https://dev-modelbank-certificate-generator.cloud.adorsys.de",
    "secure": false,
    "changeOrigin": true,
    "logLevel": "debug",
    "pathRewrite": {
      "^/certgen": ""
    }
  },

  "/tpp/*": {
    "target": "https://dev-modelbank-tpp-backend.cloud.adorsys.de",
    "secure": false,
    "changeOrigin": true,
    "logLevel": "debug",
    "pathRewrite": {
      "^/tpp": "/tpp"
    }
  }
}
